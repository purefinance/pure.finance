{
  "name": "pure-finance-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "deps:check": "dependency-check --detective precinct --ignore-module autoprefixer --ignore-module patch-package --ignore-module react-dom --ignore-module tailwindcss --no-dev pages/*",
    "postinstall": "patch-package",
    "prestart": "npm run build",
    "start": "next start"
  },
  "dependencies": {
    "@web3-react/core": "^6.1.1",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.1.9",
    "autoprefixer": "^10.2.4",
    "big.js": "^6.0.3",
    "erc-20-lib": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^10.0.7",
    "next-translate": "^1.0.6",
    "patch-package": "^6.4.7",
    "pure-finance-lib": "^1.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-modal": "^3.12.1",
    "tailwindcss": "^2.0.3",
    "vesper-metadata": "^2.0.0",
    "web3": "^1.3.0",
    "web3-utils": "^1.3.4"
  },
  "devDependencies": {
    "serve": "^11.3.2"
  },
  "babel": {
    "presets": [
      [
        "next/babel",
        {
          "preset-env": {
            "useBuiltIns": false,
            "targets": "defaults, not ie 11"
          }
        }
      ]
    ]
  }
}
