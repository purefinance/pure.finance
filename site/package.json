{
  "name": "pure-finance-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently --kill-others --names \"Fork,Next\" \"node utils/fork.js\" \"next dev\"",
    "deps:check": "dependency-check --detective precinct --ignore-module autoprefixer --ignore-module react-dom --ignore-module tailwindcss --ignore-module next-secure-headers --ignore-module eslint-config-next --no-dev ./pages/*.js ./pages/*/*.js ./pages/*/*/*.js ./pages/*/*/*/*.js",
    "generate:htaccess": "node ./scripts/generateHeaders.js",
    "postbuild": "npm run generate:htaccess",
    "preserve": "npm run build",
    "serve": "serve out"
  },
  "dependencies": {
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/walletlink-connector": "6.2.8",
    "autoprefixer": "10.4.21",
    "big.js": "6.1.1",
    "dp-auctions-lib": "1.0.0",
    "erc-20-lib": "1.0.0",
    "eslint-config-next": "14.1.3",
    "hemi-socials": "1.0.0",
    "lodash.debounce": "4.0.8",
    "lodash.orderby": "4.6.0",
    "luxon": "2.1.1",
    "next": "14.2.32",
    "next-intl": "3.9.4",
    "next-secure-headers": "2.2.0",
    "pf-payment-streams": "1.0.0",
    "pure-finance-lib": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-tooltip": "4.2.21",
    "swr": "2.2.5",
    "tailwindcss": "3.4.18",
    "token-list": "1.0.0",
    "victory": "36.9.2",
    "wallet-watch-asset": "github:hemilabs/wallet-watch-asset#v1.0.0",
    "web3": "1.10.4",
    "web3-utils": "1.10.4"
  },
  "devDependencies": {
    "concurrently": "6.4.0",
    "dotenv": "10.0.0",
    "ganache-core": "2.13.2",
    "serve": "14.2.1"
  },
  "babel": {
    "presets": [
      [
        "next/babel",
        {
          "preset-env": {
            "useBuiltIns": false,
            "targets": "defaults, not ie 11"
          }
        }
      ]
    ]
  }
}
